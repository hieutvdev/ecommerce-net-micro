{
  "AzureAdB2C": {
    "Instance": "https://qualified.domain.name.b2clogin.com/",
    "ClientId": "11111111-1111-1111-11111111111111111",
    "Domain": "qualified.domain.name",
    "Scopes": "access_as_user",
    "SignUpSignInPolicyId": "b2c_1_susi"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "restrictedToMinimumLevel": "Verbose"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "shared": true,
          "restrictedToMinimumLevel": "Error"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "MessageBroker": {
    "Host": "amqp://localhost:5672",
    "UserName": "dinobaby",
    "Password": "26112003"
  },
  "FeatureManagement": {
    "InventoryFullFigment": false
  },
  "ConnectionStrings": {
    "Database": "Server=localhost;Port=5004;Database=inventory-db;User Id=postgres;Password=26112003;Include Error Detail=true"
  },
  "RedisConfiguration": {
    "Enabled": true,
    "ConnectionStrings": "localhost:6000"
  }
}
